{
  "name": "@vuei/nuxt",
  "version": "1.0.31",
  "description": "Innovative Vue UI Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/germondai/vuei.git"
  },
  "author": "germondai",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "bunx nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "bun lint && bun test && bun prepack && changelogen --release && bun publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write --list-different .",
    "fmt": "bun lint:fix && bun prettier:fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@germondai/ts-utils": "^0.0.1",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/kit": "^3.15.4",
    "@nuxtjs/tailwindcss": "^6.13.1",
    "@vueuse/core": "^12.7.0",
    "@vueuse/motion": "^2.2.6",
    "clsx": "^2.1.1",
    "defu": "^6.1.4",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.1.0",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.0",
    "@types/node": "^22.13.5",
    "changelogen": "^0.6.0",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "prettier": "^3.5.2",
    "typescript": "~5.7.3",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
  }
}
